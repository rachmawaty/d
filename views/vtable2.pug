script.
    $(document).ready(function() {
        $('#tabledataset').DataTable({
            "pageLength": 1,
            "searching": false,
            "ordering": false,
            "lengthChange": false,
            "scrollX": 720,
            "scrollY": 480
        });
        var tableinsides = document.getElementsByClassName('cell-border');
        console.log(tableinsides.length);
        $('#tableinside').DataTable({
            "pageLength": 5
        });
    } );

div#table.tabcontent
    if (ds)
        table#tabledataset.display(style="width:100%")
            thead
                tr
                    th
            tbody
                each table in ds
                    tr
                        td
                            if (table.dataset.title)
                                h4 #{table.dataset.title.value}
                            else 
                                h4 Combined Table
                            table#tableinside.cell-border
                                thead
                                    tr
                                        each attr in table.headers
                                            th #{attr}
                                tbody
                                    each row in table.bindings
                                        tr
                                            each attr in table.headers
                                                td
                                                    if (row[attr].type == 'uri')
                                                        a(href=row[attr].value) #{row[attr].value}
                                                    else
                                                        | #{row[attr].value}